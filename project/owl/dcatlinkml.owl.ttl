@prefix PATO: <http://purl.obolibrary.org/obo/PATO_> .
@prefix dcatlinkml: <https://w3id.org/HendrikBorgelt/DCATlinkML/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix linkml: <https://w3id.org/linkml/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema1: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dcatlinkml:CatalogCollection a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "catalogCollection" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom dcatlinkml:Catalog ;
            owl:onProperty dcatlinkml:entries ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty dcatlinkml:entries ] ;
    skos:definition "A holder for catalog objects" ;
    skos:inScheme <https://w3id.org/HendrikBorgelt/DCATlinkML> .

dcatlinkml:Catalog a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "catalog" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty dcatlinkml:vital_status ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Integer ;
            owl:onProperty dcatlinkml:age_in_years ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty dcatlinkml:primary_email ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty dcatlinkml:birth_date ],
        [ a owl:Restriction ;
            owl:allValuesFrom dcatlinkml:PersonStatus ;
            owl:onProperty dcatlinkml:vital_status ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty dcatlinkml:vital_status ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty dcatlinkml:primary_email ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty dcatlinkml:birth_date ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a rdfs:Datatype ;
                    owl:onDatatype xsd:string ;
                    owl:withRestrictions ( [ xsd:pattern "^\\S+@[\\S+\\.]+\\S+" ] ) ] ;
            owl:onProperty dcatlinkml:primary_email ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty dcatlinkml:age_in_years ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:Date ;
            owl:onProperty dcatlinkml:birth_date ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty dcatlinkml:age_in_years ],
        dcatlinkml:NamedThing ;
    skos:definition "Represents a catalog" ;
    skos:inScheme <https://w3id.org/HendrikBorgelt/DCATlinkML> .

dcatlinkml:NamedThing a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "NamedThing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom linkml:Uriorcurie ;
            owl:onProperty dcatlinkml:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty dcatlinkml:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom linkml:String ;
            owl:onProperty dcatlinkml:name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty dcatlinkml:description ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty dcatlinkml:name ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty dcatlinkml:name ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty dcatlinkml:id ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty dcatlinkml:id ],
        [ a owl:Restriction ;
            owl:minCardinality 0 ;
            owl:onProperty dcatlinkml:description ] ;
    skos:definition "A generic grouping for any identifiable entity" ;
    skos:exactMatch schema1:Thing ;
    skos:inScheme <https://w3id.org/HendrikBorgelt/DCATlinkML> .

PATO:0001421 a owl:Class,
        dcatlinkml:PersonStatus ;
    rdfs:label "ALIVE" ;
    rdfs:subClassOf dcatlinkml:PersonStatus .

PATO:0001422 a owl:Class,
        dcatlinkml:PersonStatus ;
    rdfs:label "DEAD" ;
    rdfs:subClassOf dcatlinkml:PersonStatus .

<https://w3id.org/HendrikBorgelt/DCATlinkML/PersonStatus#UNKNOWN> a owl:Class,
        dcatlinkml:PersonStatus ;
    rdfs:label "UNKNOWN" ;
    rdfs:subClassOf dcatlinkml:PersonStatus .

dcatlinkml:entries a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "entries" ;
    skos:inScheme <https://w3id.org/HendrikBorgelt/DCATlinkML> .

dcatlinkml:age_in_years a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "age_in_years" ;
    rdfs:range linkml:Integer ;
    skos:definition "Number of years since birth" ;
    skos:inScheme <https://w3id.org/HendrikBorgelt/DCATlinkML> .

dcatlinkml:birth_date a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "birth_date" ;
    rdfs:range linkml:Date ;
    skos:definition "Date on which a person is born" ;
    skos:inScheme <https://w3id.org/HendrikBorgelt/DCATlinkML> .

dcatlinkml:description a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "description" ;
    skos:definition "A human-readable description for a thing" ;
    skos:inScheme <https://w3id.org/HendrikBorgelt/DCATlinkML> .

dcatlinkml:id a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range linkml:Uriorcurie ;
    skos:definition "A unique identifier for a thing" ;
    skos:inScheme <https://w3id.org/HendrikBorgelt/DCATlinkML> .

dcatlinkml:name a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "name" ;
    skos:definition "A human-readable name for a thing" ;
    skos:inScheme <https://w3id.org/HendrikBorgelt/DCATlinkML> .

dcatlinkml:primary_email a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "primary_email" ;
    skos:definition "The main email address of a person" ;
    skos:inScheme <https://w3id.org/HendrikBorgelt/DCATlinkML> .

dcatlinkml:vital_status a owl:ObjectProperty,
        linkml:SlotDefinition ;
    rdfs:label "vital_status" ;
    rdfs:range dcatlinkml:PersonStatus ;
    skos:definition "living or dead status" ;
    skos:inScheme <https://w3id.org/HendrikBorgelt/DCATlinkML> .

dcatlinkml:PersonStatus a owl:Class,
        linkml:EnumDefinition ;
    owl:unionOf ( PATO:0001421 PATO:0001422 <https://w3id.org/HendrikBorgelt/DCATlinkML/PersonStatus#UNKNOWN> ) ;
    linkml:permissible_values PATO:0001421,
        PATO:0001422,
        <https://w3id.org/HendrikBorgelt/DCATlinkML/PersonStatus#UNKNOWN> .

<https://w3id.org/HendrikBorgelt/DCATlinkML> a owl:Ontology ;
    rdfs:label "DCATlinkML" ;
    dcterms:license "MIT" ;
    dcterms:title "DCATlinkML" ;
    rdfs:seeAlso <https://HendrikBorgelt.github.io/DCATlinkML> ;
    skos:definition "test" .

